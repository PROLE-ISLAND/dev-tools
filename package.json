{
  "name": "@prole-island/dev-tools",
  "version": "0.2.0",
  "description": "PROLE-ISLAND 統合開発ツール - v0生成、リポジトリ初期化、Claude Code設定",
  "type": "module",
  "bin": {
    "prole": "./dist/index.js",
    "prole-dev": "./dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --watch",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "prole-island",
    "dev-tools",
    "v0",
    "claude-code",
    "cli"
  ],
  "author": "PROLE-ISLAND",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PROLE-ISLAND/dev-tools.git"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
